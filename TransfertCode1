Sub SupprimerBoutons()
    Dim ws As Worksheet
    Dim shp As Shape

    ' Définir la feuille spécifiée
    Set ws = ThisWorkbook.Sheets("NomDeLaFeuille") ' Remplacez "NomDeLaFeuille" par le nom de votre feuille

    ' Parcourir toutes les formes de la feuille et supprimer les boutons
    For Each shp In ws.Shapes
        If shp.Type = msoFormControl Then
            If shp.FormControlType = xlButtonControl Then
                shp.Delete
            End If
        End If
    Next shp
End Sub
