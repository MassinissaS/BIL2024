Sub ExtractMonthAndYear()
    Dim text As String
    Dim monthAndYear As String
    Dim monthName As String
    Dim yearValue As String
    Dim startPos As Integer
    
    ' Le texte de la cellule (remplacez A1 par votre cellule réelle)
    text = ThisWorkbook.Sheets("Sheet1").Range("A1").Value
    
    ' Trouver la position de "de :" pour commencer l'extraction
    startPos = InStr(text, "de :") + Len("de :")
    
    ' Extraire le texte après "de :"
    monthAndYear = Trim(Mid(text, startPos))
    
    ' Séparer le mois et l'année
    monthName = Split(monthAndYear)(0)
    yearValue = Split(monthAndYear)(1)
    
    ' Afficher les résultats (ou les utiliser dans votre code)
    MsgBox "Mois: " & monthName & vbCrLf & "Année: " & yearValue
End Sub
