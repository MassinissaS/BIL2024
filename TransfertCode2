Function GetTextBeforeParenthesis(cellValue As String) As String
    Dim position As Integer
    
    ' Trouver la position de la première parenthèse ouvrante
    position = InStr(1, cellValue, "(")
    
    ' Si une parenthèse est trouvée, extraire le texte avant celle-ci
    If position > 0 Then
        GetTextBeforeParenthesis = Left(cellValue, position - 1)
    Else
        ' Si aucune parenthèse n'est trouvée, retourner le texte complet
        GetTextBeforeParenthesis = cellValue
    End If
End Function
